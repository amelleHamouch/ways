<Page x:Class="Ways.Vues.WaysHomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Ways.Vues"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WaysHomePage" >

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
            <Button x:Name="AccessUserBtn" Content="Espace Candidat" Background="#448085" Height="273" Foreground="White" FontSize="25" Width="264" Margin="50" Click="Button_UserAccess" MinWidth="100"/>
            <Button x:Name="AccessAdminBtn" Content="Administrateur CESI" Background="#448085" Foreground="White" Height="268" FontSize="25" Margin="50" MinWidth="100" Width="264" Click="Button_AdminAccess">
                <Button.Effect>
                    <DropShadowEffect/>
                </Button.Effect>
            </Button>
        </StackPanel>
    </Grid>
</Page>
