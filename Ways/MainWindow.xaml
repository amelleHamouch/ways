<Window x:Class="Ways.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ways"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="35" >
            <Button x:Name="AccessUserBtn" Content="Candidat"  Width="143" Click="Button_UserAccess" MinWidth="100"/>
            <Button x:Name="AccessAdminBtn" Content="Administrateur CESI" MinWidth="100" Width="143" Click="Button_AdminAccess"/>
        </StackPanel>
        
        <Frame x:Name="Main"></Frame>
        
    </Grid>
</Window>
